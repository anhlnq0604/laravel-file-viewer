{
    "name": "anhlnq0604/laravel-file-viewer",
    "description": "A basic file viewer for Laravel projects",
    "keywords": [
        "anhlnq0604",
        "laravel-file-viewer"
    ],
    "homepage": "https://github.com/anhlnq0604/laravel-file-viewer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Le Nguyen Quang Anh",
            "email": "anhlnq0604@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Anhlnq0604\\LaravelFileViewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anhlnq0604\\LaravelFileViewer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anhlnq0604\\LaravelFileViewer\\LaravelFileViewerServiceProvider"
            ],
            "aliases": {
                "LaravelFileViewer": "Anhlnq0604\\LaravelFileViewer\\LaravelFileViewerFacade"
            }
        }
    }
}